{
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": ["error", "single"]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6" : true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}