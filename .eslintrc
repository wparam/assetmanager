{
    "plugins": [
        "react"
    ],
    "rules": {
        "prefer-destructuring": ["error", {
            "array": true,
            "object": true
        }, {
        "enforceForRenamedProperties": false
        }],
        "semi": 0,
        "quotes": ["error", "single"]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6" : true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}